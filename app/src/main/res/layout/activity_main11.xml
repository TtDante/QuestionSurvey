<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.a13456.questionsurvey.Main11Activity">

    <TextView
        android:text="10,你认为最有效的求职途径是"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp"
        android:id="@+id/textView11" />

    <CheckBox
        android:text="通过家庭和个人社会关系、托熟人"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView11"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="34dp"
        android:id="@+id/checkBox10_1" />

    <CheckBox
        android:text="通过学校提供信息和推荐"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:id="@+id/checkBox10_2"
        android:layout_below="@+id/checkBox10_1"
        android:layout_alignLeft="@+id/checkBox10_1"
        android:layout_alignStart="@+id/checkBox10_1" />

    <CheckBox
        android:text="人才招聘会和人才市场"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/checkBox10_3"
        android:layout_below="@+id/checkBox10_2"
        android:layout_alignLeft="@+id/checkBox10_2"
        android:layout_alignStart="@+id/checkBox10_2"
        android:layout_marginTop="33dp" />

    <Button
        android:text="下一个"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button10_2"
        android:layout_alignBottom="@+id/button10_1"
        android:layout_toRightOf="@+id/checkBox10_3"
        android:layout_toEndOf="@+id/checkBox10_3" />

    <Button
        android:text="上一个"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="34dp"
        android:id="@+id/button10_1"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/checkBox10_4"
        android:layout_alignStart="@+id/checkBox10_4" />

    <CheckBox
        android:text="其他"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:id="@+id/checkBox10_4"
        android:layout_below="@+id/checkBox10_3"
        android:layout_alignLeft="@+id/checkBox10_3"
        android:layout_alignStart="@+id/checkBox10_3" />
</RelativeLayout>
